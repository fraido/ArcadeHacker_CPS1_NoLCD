void ProgramCPS1(int prg) {
   int i;
   int test;

   Serial.println("Waiting 5 Secs...");
   delay(5000);
   Serial.println("Unlocking .......");                  
      
   program_unlock();      
                
   Serial.println("Programming .....");
   Serial.print  ("CPS-B-21 Keys ... ");    
                               
   for(i=0; i<18; i++) {
      Serial.print(bytes[prg][i], HEX);
      Serial.print(" ");  
      for (int b = 7; b > -1; b--) {  
        bits[b] = (bytes[prg][i] & (mask << b)) != 0;
//      printf("%d",bits[b]);
       
        if (bits[b] == 1) {
          digitalWrite(PIN62, HIGH);}
        else { 
          digitalWrite(PIN62, LOW);}                  
          PIN61CLK();  
        }
   }
   Serial.println("\nDone!");
   Serial.println("Disconnect and test pcb.");                         

   pinMode(PIN42, INPUT); pinMode(PIN44, INPUT);
   pinMode(PIN61, INPUT); pinMode(PIN62, INPUT); 
}
             

void loop() {

  ProgramCPS1(CPSB21_ID); 
  while(1);               

}
